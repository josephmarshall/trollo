<h1><%= @list.name ? "Edit " : "New " %>List</h1>
<div class="form_input"><%= form_with(model: [@board, @list], local: true) do |f| %><?div>
<div class="form_input_label"><%= f.label :name %></div>
<div class="form_input"><%= f.text_field :name %></div>
<div class="form_input_label"><%= f.label :priority %></div>
<div class="form_input"><%= f.text_field :priority %></div>
<div class="form_input"><%= f.submit %></div>
<% end %>
</hr>